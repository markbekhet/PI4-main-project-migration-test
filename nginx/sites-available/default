server {
    listen 443 ssl;
    listen [::]:443 ssl;
    include snippets/self-signed.conf;
    include snippets/ssl-params.conf;  

    location / {
        proxy_pass  http://127.0.0.1:5000;
    }
}
server {
    listen 80;
    listen [::]:80;
    server_name _;

    return 302 https://127.0.0.1:5000$request_uri;
}
